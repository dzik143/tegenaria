  Keywords1   p386,.386,p486,.486,p586,.586,.586p,.data,dataseg,.code
  Keywords1   codeseg,.udata,udataseg,short,offset,near,end,struc

; Registers and size related
  KeyWords3   db,dw,dd,dq,dt,tiny,large,word,dword,byte,ptr
  RegKeywords Color lightgray \b[abcd][lh]\b, \be?[abcd]x\b, \bmm[0-7]\b
  RegKeywords Color lightgray \b[cdefgs]s\b, \be?[ds]i\b, \be?[bs]p\b
